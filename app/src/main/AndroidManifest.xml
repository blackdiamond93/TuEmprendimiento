<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tuemprendimiento">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_pensamiento"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_pensamiento"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity
            android:name=".Activity.DetallesActivity"
            android:label="@string/title_activity_detalles"
            android:parentActivityName=".Activity.EmprendimientoActivity"
            android:theme="@style/NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.tuemprendimiento.Activity.EmprendimientoActivity" />
        </activity>
        <activity
            android:name=".Activity.MenuActivity"
            android:label="@string/title_activity_main2"
            android:theme="@style/NoActionBar"><intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>

        </activity>
        <activity
            android:name=".Activity.EmprendimientoActivity"
            android:label="@string/title_activity_emprendimiento"
            android:parentActivityName=".Activity.MainActivity"
            android:theme="@style/NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.tuemprendimiento.Activity.MainActivity" />
        </activity>
    </application>

</manifest>